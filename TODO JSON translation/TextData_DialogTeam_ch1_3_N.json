"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 352301,
"ZH_Text0": "快！走吧！",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Nhanh! Đi thôi!",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 352302,
"ZH_Text0": "真礙事。",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Thật bất tiện.",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 352303,
"ZH_Text0": "交還神女！",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Trả lại thần nữ!",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 352304,
"ZH_Text0": "接著換你！",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Sau đó sẽ đổi người!",
"CH_Text1": "",
"CH_Text2": "",
