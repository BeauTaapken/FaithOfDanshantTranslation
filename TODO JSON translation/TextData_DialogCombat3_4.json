"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 480209,
"ZH_Text0": "",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 480210,
"ZH_Text0": "",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 480211,
"ZH_Text0": "寒霜凍結此區域，速度持續下降",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Sương lạnh đông kết khu vực này, tốc độ tiếp tục giảm",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 480212,
"ZH_Text0": "寒霜即將到達極限，趕緊離開此區域！",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Sương lạnh sắp đến cực hạn, mau chóng rời khỏi khu vực này!",
"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 480213,
"ZH_Text0": "完全凍結，無法動彈！",
"ZH_Text1": "",
"ZH_Text2": "",
"CH_Text0": "Hoàn toàn đông kết, không cách nào động đậy!",
