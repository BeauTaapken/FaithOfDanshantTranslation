"CH_Text1": "",
"CH_Text2": "",
"EN_Text0": "",
"EN_Text1": "",
"EN_Text2": "",
"JP_Text0": "",
"JP_Text1": "",
"JP_Text2": ""
},
{
"ID": 231108,
"ZH_Text0": "",
"ZH_Text1": "",
